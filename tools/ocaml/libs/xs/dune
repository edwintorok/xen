; fallback mode: the files may have been generated by configure already
(rule
 (targets  paths.ml)
 (deps  paths.ml.in)
 (mode fallback)
 (action (run ../../../config.status --file=paths.ml)))

(library
  ; avoid conflict with mirage lib: name it differently
 (name xenstore_xen)
 (public_name xen.store)
 (no_dynlink)
 (libraries unix xenbus))
